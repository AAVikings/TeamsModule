    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "fast-xml-parser": "^3.12.12",
    "graphql": "^14.1.1",
    "graphql-import": "^0.7.1",
    "graphql-middleware": "^1.7.8",
