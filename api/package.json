    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "debug": "^4.1.1",
    "graphql-cli": "^3.0.9",
    "nodemon": "^1.18.10",
    "npm-run-all": "4.1.5",
    "prisma": "^1.26.4"
